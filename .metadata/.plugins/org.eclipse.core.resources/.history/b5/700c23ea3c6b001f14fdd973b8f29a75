package controller;

public class ThreadVetor extends Thread{
	private int vetor[];
	private int numero;
	
	
	public ThreadVetor(int vet[], int num) {
		vetor = vet;
		numero = num;
	}
	
	public void run() {
		double tempoInicial = System.nanoTime();
		int valor = 0;
		
		if(numero % 2 == 0) {
			for(int i = 0; i< vetor.length; i++) {
				valor = vt[i];
			}
		}
	}
	
	
	
}
